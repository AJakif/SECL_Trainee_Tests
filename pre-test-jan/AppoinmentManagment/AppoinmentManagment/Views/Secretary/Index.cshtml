
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-center text-primary">Secretary Dashboard</h1>
<br />
<div class="row col-12">
    <div class="border backgroundWhite col-lg-8">
        <h2 class="text-center">All Appoinments</h2>
        <br />
        <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="th-sm text-center">Patient Name</th>
                    <th class="th-sm text-center">Phone</th>
                    <th class="th-sm text-center">Address</th>
                    <th class="th-sm text-center">Date of Birth</th>
                    <th class="th-sm text-center">Action</th>
                </tr>
            </thead>
            <tbody id="tableData">
            </tbody>
        </table>
    </div>
</div>

